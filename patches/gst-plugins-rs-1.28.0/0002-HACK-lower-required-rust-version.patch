@@ -124,7 +124,7 @@ categories = ["api-bindings", "multimedia"]
 repository = "https://gitlab.freedesktop.org/gstreamer/gstreamer-rs"
 homepage = "https://gstreamer.freedesktop.org"
 edition = "2024"
-rust-version = "1.92"
+rust-version = "1.91"
 
 [workspace.dependencies]
 gio = { git = "https://github.com/gtk-rs/gtk-rs-core", branch = "main", version = "0.22.0-alpha" }
diff --git a/ptxdist-cargo-home/workspaces/gtk-rs-core-0.22.0-alpha.2+19d547784bb7/Cargo.toml b/ptxdist-cargo-home/workspaces/gtk-rs-core-0.22.0-alpha.2+19d547784bb7/Cargo.toml
